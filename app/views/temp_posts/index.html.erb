<h1>TempPosts index</h1>

<table>
  <tr>
    <td>ID #</td>
    <td>Title</td>
    <td>Original Post</td>
  </tr>

<% for post in @temp_posts %>
  <tr>
    <td><%= post.id %></td>
    <td><%= link_to post.title, post %></td>
    <td><%= post.post_id %> </td>
    <td>
      <dt><%= link_to 'Edit', edit_temp_post_path(post) %></dt>
      <dt><%= link_to 'Destroy', post, :confirm => 'Are you sure?', :method => :delete %></dt>
    </td>
  </tr>
<% end %>
</table>

<br />

